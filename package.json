{
  "name": "gulp-wp-rev",
  "version": "0.0.1",
  "description": "Revisioning JS and CSS to scripts.php file on wordpress assets",
  "keywords": [
    "gulpplugin",
    "wordpress"
    "rev",
    "revision",
    "hash",
    "optimize",
    "version",
    "versioning",
    "cache",
    "expire",
    "static",
    "asset",
    "assets"
  ],
  "homepage": "https://github.com/raulghm/gulp-wp-rev",
  "bugs": "https://github.com/raulghm/gulp-wp-rev/issues",
  "author": {
    "name": "Raul Hernandez",
    "email": "raulghm@gmail.com",
    "url": "https://github.com/raulghm"
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/raulghm/gulp-wp-rev.git"
  },
  "scripts": {
    "test": "istanbul test _mocha --report html -- test/*.js --reporter spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "through2": "*",
    "gulp-util": "~2.2.0"
  },
  "devDependencies": {
    "mocha": "*",
    "coveralls": "*",
    "mocha-lcov-reporter": "*",
    "istanbul": "*",
    "event-stream": "*",
    "should": "~2.1.0"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.2.10"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ]
}
